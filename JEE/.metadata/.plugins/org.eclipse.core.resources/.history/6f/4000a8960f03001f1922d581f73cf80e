package com.jsp;

import java.util.Scanner;

public class MainClass {
	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		System.out.println("enter num");
		int org=sc.nextInt();
		int num=org;
		int count=0;
		while(num>0) {
			num=num/10;
			count++;
		}
		System.out.println("digit: "+count);
		
		
	}
}
