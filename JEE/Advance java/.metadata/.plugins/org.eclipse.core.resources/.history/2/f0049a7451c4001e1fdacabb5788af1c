package com.java;
import java.util.Scanner;
import java.sql.*;
public class Practice {
	
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter Number");
		int org=sc.nextInt();
		int num=org;
		int flag=0;
		if(org==1||org==0) {
			System.out.println("not prime no.");
		}
		else {
			for(int i=2;i<num;i++) {
				if(num%i==0) {
					System.out.println("not prime no.");
					flag=1;
					break;
				}
				if(flag==0) {
					System.out.println("prime");
				}
			}
			
		}
				
		
	}
}
