package com.java;

import java.sql.*;
import org.postgresql.Driver;

public class RegisterJdbc {

	public static void main(String[] args) {
		
		try {
			Driver driver=new Driver();
			DriverManager.registerDriver(driver);
			
			
		} catch (SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
		
	}

}
