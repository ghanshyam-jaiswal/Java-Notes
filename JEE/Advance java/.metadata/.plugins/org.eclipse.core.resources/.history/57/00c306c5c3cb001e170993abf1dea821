package com.java;

import java.util.Arrays;
import java.util.Scanner;
public class Practice {
	
	public static void main(String[] args) {
		String s="shyam007";
		char[] c=s.toCharArray();
				
				
		int sum=0;
		for(int i=0;i<s.length();i++) {
			if(Character.isDigit(c[i])){
				sum=sum+c[i];
			}
		}
		System.out.println(sum);
		
				
	}
}

