package com.java;

import java.util.Arrays;
import java.util.Scanner;
public class Practice {
	
	public static void main(String[] args) {
		String s="ghanshyam";
		int count=0;
		for(int i=s.length()-1;i>=0;i--) {
			System.out.print(s.charAt(i));
			count++;
		}
		System.out.println(" "+count);
		
		
	}
}

