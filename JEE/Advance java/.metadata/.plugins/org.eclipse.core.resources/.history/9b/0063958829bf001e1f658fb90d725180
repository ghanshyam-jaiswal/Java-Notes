package com.java;

import java.io.File;
import java.io.FileReader;

public class FileExample {

	public static void main(String[] args) {

		File file=new File("a.txt");
		
		try {
			FileReader fw= FileReader(file);
			for(int i=fw.read();i!=-1;i++) {
				char c=(char)fw.read();
				System.out.println(c);
			}
					
		}catch (Exception e) {

		}
	}


}
