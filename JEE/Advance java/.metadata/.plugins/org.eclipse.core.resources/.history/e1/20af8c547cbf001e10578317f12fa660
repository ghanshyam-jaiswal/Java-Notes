package com.java;
import java.util.Scanner;
public class Practice {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter Number");
		int org=sc.nextInt();
		
		int mul=1;
		for(int i=1;i<=org;i++) {
			mul=mul*i;
		}
		System.out.println("Fectorial of "+org+" is : "+mul);
		
		
		
		
	}
}
