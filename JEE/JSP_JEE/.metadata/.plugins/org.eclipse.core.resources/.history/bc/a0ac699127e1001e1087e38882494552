		int upper=0;
		int lower=0;
		int other=0;
		for(int i=0;i<c.length;i++) {
			if((c[i]>='a'||c[i]>='A') && (c[i]<='z'||c[i]<='Z')) {
				if(Character.isUpperCase(c[i])) {
					upper++;
				}else {
					lower++;
				}
			}
			else {
				other++;
			}
		}System.out.println("upper:"+upper);
		System.out.println("lower:"+lower);
		System.out.println("other:"+other);
		
		
	}
